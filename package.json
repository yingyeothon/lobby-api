{
  "name": "yyt-lobby",
  "version": "1.0.0",
  "description": "A simple Serverless backend for lobby",
  "main": "src/handler.ts",
  "scripts": {
    "start": "sls offline",
    "test": "SLACK_LOG_LEVEL=silent CONSOLE_LOG_LEVEL=trace jest --runInBand --detectOpenHandles",
    "build": "node --max-old-space-size=4096 node_modules/serverless/bin/serverless package",
    "deploy": "node --max-old-space-size=4096 node_modules/serverless/bin/serverless deploy",
    "analyze": "ANALYZE_BUNDLE=1 yarn build"
  },
  "dependencies": {
    "@yingyeothon/actor-system": "^0.3.1",
    "@yingyeothon/actor-system-redis-support": "^0.3.0",
    "@yingyeothon/naive-redis": "^0.3.2",
    "@yingyeothon/slack-logger": "^1.0.1",
    "jsonwebtoken": "^9.0.0",
    "mem": "^8.1.1",
    "p-limit": "^3.1.0",
    "p-memoize": "^4.0.1",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.40.0",
    "@types/aws-lambda": "^8.10.76",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "15",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "aws-sdk": "2.804.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "serverless": "^2.41.2",
    "serverless-offline": "^7.0.0",
    "serverless-prune-plugin": "^1.5.0",
    "serverless-webpack": "^5.5.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "wscat": "^4.0.1"
  },
